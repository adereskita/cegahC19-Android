@@ -0,0 +1,56 @@
+package org.com.application;
+
+import android.content.Context;
+import android.view.LayoutInflater;
+import android.view.View;
+import android.view.ViewGroup;
+import android.widget.TextView;
+
+import androidx.annotation.NonNull;
+import androidx.recyclerview.widget.RecyclerView;
+
+import java.util.ArrayList;
+
+public class RecyclerViewGejalaAdapter extends RecyclerView.Adapter<RecyclerViewGejalaAdapter.ViewHolder> {
+
+    private ArrayList<UserModel> data;
+    private Context mContext;
+
+    public RecyclerViewGejalaAdapter(ArrayList<UserModel> data, Context mContext) {
+        this.data = data;
+        this.mContext = mContext;
+    }
+
+    @NonNull
+    @Override
+    public RecyclerViewGejalaAdapter.ViewHolder onCreateViewHolder(@NonNull ViewGroup parent, int viewType) {
+        LayoutInflater inflater = LayoutInflater.from(mContext);
+        View view = inflater.inflate(R.layout.list_riwayat,parent,false);
+        return new ViewHolder(view);
+    }
+
+    @Override
+    public void onBindViewHolder(@NonNull RecyclerViewGejalaAdapter.ViewHolder holder, int position) {
+        UserModel user = data.get(position);
+
+        holder.no.setText(user.getId());
+    }
+
+    @Override
+    public int getItemCount() {
+        return data.size();
+    }
+
+    public class ViewHolder extends RecyclerView.ViewHolder{
+        private TextView no, tanggalgejala, gejala;
+
+        public ViewHolder(@NonNull View itemView) {
+
+            super(itemView);
+            no = itemView.findViewById(R.id.no_riwayat);
+            tanggalgejala = itemView.findViewById(R.id.tanggal_riwayat);
+            gejala = itemView.findViewById(R.id.gejala_riwayat);
+
+        }
+    }
+}
